#import "UserModel.h"
static UserModel *shareUser = nil;
@implementation UserModel
+(id)shareuser{
    if (shareUser == nil) {
        shareUser =[[UserModel alloc]init];
    }
    return shareUser;
}
-(void)initUserByDic:(NSDictionary*)dic{
    self.token = [dic objectForKey:@"token"];
    self.isLoad = YES;
}
-(void)goout{
    self.token=nil;
    self.isLoad=NO;
    self.idcard=nil;
    self.userName=nil;
    self.truename=nil;
    self.sex=nil;
    self.pwd=nil;
    self.userid=nil;
    self.money=nil;
   
}
@end
